<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <!-- header website template -->
    <template id="macnash_template_header" inherit_id="website.layout" name="Macnsash header">
        <xpath expr="//header[@id='top']" position="replace">
            <header id="top" data-anchor="true" data-name="Header" class="  o_header_standard">
                <nav data-name="Navbar" class="navbar navbar-expand-lg">
                    <div id="top_menu_container" class="container justify-content-start justify-content-lg-between">
                        <a href="/" class="navbar-brand mr-4">
                            <span role="img" aria-label="Logo of My Website" title="My Website">
                                <img src="/mac_website/static/src/img/logo.png" class="img" alt="macnash logo"/>
                            </span>
                        </a>
                        <button type="button" data-toggle="collapse" data-target="#top_menu_collapse" class="navbar-toggler ml-auto">
                            <span class="navbar-toggler-icon o_not_editable"></span>
                        </button>
                        <div id="top_menu_collapse" class="collapse navbar-collapse flex-column" aria-expanded="false">
                            <ul class="nav align-self-end navbar-top">
                                <li id="whiseAreaZone"><a id="whiseAreaLink" class="text-capitalize" href="https://owner-whise.webulous.be/3J2he3F9Nb" target="_blank"><i class="fa fa-cloud fa-left"></i>Owner area</a></li>
                                <li>
                                    <div class="dropdown">
                                        <a data-target="#" data-toggle="dropdown" role="button"><i class="fa fa-cog fa-left"></i>EN</a>
                                        <ul class="dropdown-menu"><li class="F"><a href="/fr/contact.aspx">FR</a></li><li class="M"><a href="/nl/contact.aspx">NL</a></li><li class="LS active"><a href="/en/contact.aspx">EN</a></li></ul>
                                    </div>
                                </li>
                            </ul>
                            <ul id="top_menu" class="nav navbar-nav flex-grow-1">
                                <li class="nav-item">
                                    <a role="menuitem" href="/" class="nav-link ">
                                        <span>free valuation</span>
                                    </a>
                                </li>
                                <li class="nav-item d-flex align-items-center">
                                    <div class="btn-group">
                                        <button type="button" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                            our agencies
                                        </button>
                                        <div class="dropdown-menu">
                                            <a class="dropdown-item" href="#">Macnash SUD</a>
                                            <a class="dropdown-item" href="#">Macnash BRABANT</a>
                                        </div>
                                    </div>
                                </li>
                                <li class="nav-item d-flex align-items-center">
                                    <div class="btn-group">
                                        <button type="button" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                            property management
                                        </button>
                                        <div class="dropdown-menu">
                                            <a class="dropdown-item" href="#">Macnash GESTION</a>
                                            <a class="dropdown-item" href="#">Macnash SYNDIC</a>
                                        </div>
                                    </div>
                                </li>
                                <li class="nav-item">
                                    <a role="menuitem" href="/" class="nav-link ">
                                        <span>new project</span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a role="menuitem" href="/contactus" class="nav-link ">
                                        <span>Contact</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>
            </header>
        </xpath>
    </template>


