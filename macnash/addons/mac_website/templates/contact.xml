<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="webiste_contact_macnash" name="Contact">
        <t t-call="website.layout">
            <div class="section-valuation container mb-5">
                <h1>Do you have a question ?</h1>
                <div class="content">
                    <div class="row">
                        <p class="mb-4">We will reply as soon as possible.</p>
                    </div>
                    <div class="details">
                        <h2>Your details</h2>
                        <hr />
                        <div class="row">
                            <div class="col-sm-4 col-md-2">
                                <div id="status" class="form-group">
                                    <label>Title</label>
                                    <select name="status" id="status" class="form-control">
                                        <option selected="selected" value="1">Sir</option>
                                        <option value="2">Madam</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-4 col-md-3">
                                <div id="firstname" class="form-group">
                                    <label>First name *</label>
                                    <input name="firstname" type="text" id="firstName" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div id="lastName" class="form-group">
                                    <label>Last name *</label>
                                    <input name="lastName" type="text" id="lastName" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-3">
                                <div id="phone" class="form-group">
                                    <label>Phone *</label>
                                    <input name="phone" type="text" id="phone" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div id="email" class="form-group">
                                    <label>E-mail *</label>
                                    <input name="email" type="text" id="email" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <div id="street" class="form-group">
                                    <label>Street</label>
                                    <input name="street" type="text" id="street" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div id="NumberGroup" class="form-group">
                                    <label>Nr / Box</label>
                                    <input name="streetNumber" type="text" id="streetNumber" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-3">
                                <div id="zipCodeGroup" class="form-group">
                                    <label>Zip code</label>
                                    <input name="Form_zipCode" type="text" id="Form_zipCode" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div id="Form_cityGroup" class="form-group">
                                    <label>City</label>
                                    <input name="Form_city" type="text" id="Form_city" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="request">
                        <h2>Your request</h2>
                        <hr />
                        <div class="row">
                            <div class="col-sm-3">
                                <div id="propertyTypeListGroup" class="form-group">

                                    <label>Property type *</label>
                                    <select name="propertyTypeList" id="propertyTypeList" class="form-control">
                                        <option value=""></option>
                                        <option value="1">Flat</option>
                                        <option value="3">House</option>
                                        <option value="4">Office</option>
                                        <option value="5">Industrial</option>
                                        <option value="6">Garage / parking</option>
                                        <option value="7">Plot</option>
                                        <option value="2">Commercial</option>

                                    </select>

                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div id="TypeListGroup" class="form-group">
                                    <label><br/></label>
                                    <select name="TypeList" id="TypeList" class="form-control">
                                        <option selected="selected" value=""></option>
                                        <option value="0">for sale</option>
                                        <option value="1">for rent</option>

                                    </select>

                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div id="officeListGroup" class="form-group">

                                    <label>Contact agency *</label>
                                    <select name="officeList" id="officeList" class="form-control">
                                        <option value="3296">Macnash</option>
                                        <option value="1749">Macnash SUD</option>
                                        <option value="1753">Macnash MONTGOMERY</option>
                                        <option value="1755">Macnash BRABANT</option>
                                        <option value="4538">Macnash NORTH</option>
                                        <option value="6201">Macnash LEMBEEK</option>
                                        <option value="SYNDIC">Macnash SYNDIC</option>
                                        <option value="GESTION">Macnash GESTION</option>

                                    </select>

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <div id="subjectListGroup" class="form-group">

                                    <label>Subject *</label>
                                    <select name="subjectList" id="subjectList" class="aspNetDisabled form-control">
                                        <option value="0">Information request</option>
                                        <option value="1">Visit request</option>
                                        <option value="2">Rating request</option>
                                        <option value="3">Your property on sale</option>
                                        <option value="4">Renting out your property</option>
                                        <option value="5">Other</option>

                                    </select>

                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div id="Form_messageGroup" class="form-group">

                                    <label>Message</label>
                                    <textarea name="Form_message" rows="5" cols="20" id="Form_message" class="form-control" maxlength="1000"></textarea>

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="form-group">
                                    (*) Required Field
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <label class="checkbox-container">
                                    I have read and I agree the
                                    <a href="" target="_blank">privacy policy</a>
                                    including my right to access my data, to have them rectified, to have them deleted and to put an end to the processing of my data.
                                    <input id="Form_privacy" type="checkbox" name="Form_privacy" />
                                </label>
                                <div class="form-group">
                                    <a onclick="return DisableButton();" id="Form_sendButton" class="btn btn-primary" href="">Send</a>
                                    <a id="Form_disabledSendButton" class="btn btn-primary disabled" style="display:none">Send</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>
</odoo>